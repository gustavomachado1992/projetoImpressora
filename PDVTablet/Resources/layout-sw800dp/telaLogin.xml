<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#eaeaeb">
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout13"
        android:background="#eaeaeb"
        android:layout_marginBottom="1dp">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="15px"
            android:minHeight="25px"
            android:id="@+id/linearLayout15"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" />
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout14"
            android:layout_weight="1.5">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="5px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout16"
                android:layout_weight="2">
                <ImageView
                    android:src="@drawable/brasystem"
                    android:id="@+id/imageView1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="18"
                    android:layout_marginBottom="@dimen/container_logoBottom"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout11"
                android:layout_weight="4"
                android:layout_marginBottom="0.0dp">
                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:src="@drawable/brasystemcortransp"
                    android:layout_marginRight="@dimen/container_imagem"
                    android:layout_marginLeft="@dimen/container_imagem"
                    android:adjustViewBounds="false" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="10px"
            android:minHeight="10px"
            android:id="@+id/linearLayout15"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" />
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/linearLayout12"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="5px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout16"
                android:layout_weight="29" />
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:id="@+id/linearLayout1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="20">
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="285.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout2"
                    android:layout_weight="3"
                    android:layout_marginBottom="0.0dp">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout3"
                        android:layout_weight="2">
                        <LinearLayout
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout18"
                            android:layout_weight="3"
                            android:background="@drawable/bordaPretaTexto"
                            android:layout_marginBottom="2dp"
                            android:layout_marginTop="2dp">
                            <EditText
                                android:id="@+id/editLogin"
                                android:textColor="#ff404040"
                                android:inputType="textEmailAddress"
                                android:imeOptions="actionNext"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#00b4b4b4"
                                android:layout_marginLeft="3dp"
                                android:text=""
                                android:layout_weight="2"
                                android:hint="UsuÃ¡rio"
                                android:textSize="22dp" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout3"
                        android:layout_weight="2">
                        <LinearLayout
                            android:orientation="vertical"
                            android:minWidth="25px"
                            android:minHeight="25px"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout18"
                            android:layout_weight="3"
                            android:background="@drawable/bordaPretaTexto"
                            android:layout_marginTop="2dp"
                            android:layout_marginBottom="2dp">
                            <EditText
                                android:id="@+id/editSenha"
                                android:textColor="#ff404040"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:imeOptions="actionDone"
                                android:inputType="textPassword"
                                android:background="#00b4b4b4"
                                android:layout_marginLeft="3dp"
                                android:layout_weight="2"
                                android:hint="Senha"
                                android:textSize="22dp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout17"
                android:layout_weight="32" />
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout5"
                android:layout_weight="25">
                <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/btnCfg"
                    android:gravity="center"
                    android:layout_weight="16.8"
                    android:background="@drawable/standardNewButton">
                    <ImageView
                        android:src="@drawable/icactionsettings"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/asdasdasda"
                        android:layout_weight="10" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout8"
                    android:layout_weight="25.8" />
                <RelativeLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/btnLogin"
                    android:gravity="center"
                    android:layout_weight="9.8"
                    android:background="@drawable/standardNewButton">
                    <TextView
                        android:text="Login"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/txtLogin"
                        android:gravity="center"
                        android:textColor="#ff404040"
                        android:textSize="25dp" />
                    <LinearLayout
                        android:orientation="horizontal"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/linearLayout19"
                        android:gravity="center">
                        <ProgressBar
                            android:id="@+id/loader"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="5px"
                android:minHeight="25px"
                android:id="@+id/linearLayout16"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="29" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="15px"
            android:minHeight="25px"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout15" />
    </LinearLayout>
</LinearLayout>